{
  "$schema": "http://json.schemastore.org/solidaritySchema",
  "requirements": {
    "PNPM": [
      {
        "rule": "cli",
        "binary": "pnpm",
        "semver": "^10.7.0",
        "version": "--version",
        "error": "You have pnpm@{{installedVersion}}, and need pnpm@{{wantedVersion}}. Fix with `pnpm self-update`",
        "fix": "pnpm self-update"
      }
    ],
    "Node": [
      {
        "rule": "cli",
        "binary": "node",
        "semver": "^20.17.0",
        "version": "--version",
        "error": "You have node@{{installedVersion}}, and need node@{{wantedVersion}}. Go to https://nodejs.org/ to update`"
      }
    ]
  }
}